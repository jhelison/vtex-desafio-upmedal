{
    "store.home": {
        "blocks": ["tab-layout#home"]
    },
    "tab-layout#home": {
        "children": ["tab-list#home", "tab-content#home"],
        "props": {
            "blockClass": "home",
            "defaultActiveTabId": "todos"
        }
    },
    //Tab layout -------------------------------
    "tab-list#home": {
        //Todo: avoid break on responsive
        "children": [
            "tab-list.item#todos",
            "tab-list.item#promocao",
            "tab-list.item#corinthians",
            "tab-list.item#divertidos",
            "tab-list.item#guerreiros",
            "tab-list.item#palmeiras",
            "tab-list.item#signos"
        ]
    },
    //Tab list-----------
    "tab-list.item#todos": {
        "props": {
            "tabId": "todos",
            "label": "Todos",
            "defaultActiveTab": true
        }
    },
    "tab-list.item#promocao": {
        "props": {
            "tabId": "promocao",
            "label": "Promoção",
            "defaultActiveTab": true
        }
    },
    "tab-list.item#corinthians": {
        "props": {
            "tabId": "corinthians",
            "label": "CORINTHIANS"
        }
    },
    "tab-list.item#divertidos": {
        "props": {
            "tabId": "divertidos",
            "label": "DIVERTIDOS"
        }
    },
    "tab-list.item#guerreiros": {
        "props": {
            "tabId": "guerreiros",
            "label": "GUERREIROS"
        }
    },
    "tab-list.item#palmeiras": {
        "props": {
            "tabId": "palmeiras",
            "label": "PALMEIRAS"
        }
    },
    "tab-list.item#signos": {
        "props": {
            "tabId": "signos",
            "label": "SIGNOS"
        }
    },
    //Tab content-----------
    "tab-content#home": {
        "children": [
            "tab-content.item#todos",
            "tab-content.item#promocao",
            "tab-content.item#corinthians",
            "tab-content.item#divertidos",
            "tab-content.item#guerreiros",
            "tab-content.item#palmeiras",
            "tab-content.item#signos"
        ]
    },
    "tab-content.item#todos": {
        "children": [
            "flex-layout.row#slider",
            "search-result-layout.customQuery#todos"
        ],
        "props": {
            "tabId": "todos"
        }
    },
    "tab-content.item#promocao": {
        "children": [
            "flex-layout.row#slider",
            "search-result-layout.customQuery#promocao"
        ],
        "props": {
            "tabId": "promocao"
        }
    },
    "tab-content.item#corinthians": {
        "children": [
            "flex-layout.row#slider",
            "search-result-layout.customQuery#corinthians"
        ],
        "props": {
            "tabId": "corinthians"
        }
    },
    "tab-content.item#divertidos": {
        "children": [
            "flex-layout.row#slider",
            "search-result-layout.customQuery#divertidos"
        ],
        "props": {
            "tabId": "divertidos"
        }
    },
    "tab-content.item#guerreiros": {
        "children": [
            "flex-layout.row#slider",
            "search-result-layout.customQuery#guerreiros"
        ],
        "props": {
            "tabId": "guerreiros"
        }
    },
    "tab-content.item#palmeiras": {
        "children": [
            "flex-layout.row#slider",
            "search-result-layout.customQuery#palmeiras"
        ],
        "props": {
            "tabId": "palmeiras"
        }
    },
    "tab-content.item#signos": {
        "children": [
            "flex-layout.row#slider",
            "search-result-layout.customQuery#signos"
        ],
        "props": {
            "tabId": "signos"
        }
    }
}
